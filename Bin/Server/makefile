.PHONY: all
all: clean create fill

.PHONY: create
create:
	cat create.sql | sqlite3 -echo -bail server.db

.PHONY: fill
fill: create
# cat fill.sql | sqlite3 -echo -bail server.db
	./fill.bat

.PHONY : clean
clean:
	rm -f server.db
	rm -f banks.db

