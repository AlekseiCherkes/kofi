.PHONY: all
all: fill

.PHONY: create
create: clean
	cat create.sql | sqlite3 -echo -bail client.db

.PHONY: fill
fill: create
	cat fill.sql | sqlite3 -echo -bail client.db

.PHONY : clean
clean:
	rm -f client.db
#	rm -f banks.db

